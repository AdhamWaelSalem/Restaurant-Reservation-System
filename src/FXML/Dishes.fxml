<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXML.Dishes">
   <children>
      <TableView fx:id="DishesView" prefHeight="400.0" prefWidth="500.0">
         <columns>
            <TableColumn fx:id="Name" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="Price" prefWidth="75.0" text="Price" />
            <TableColumn fx:id="Amount" prefWidth="75.0" text="Amount" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
