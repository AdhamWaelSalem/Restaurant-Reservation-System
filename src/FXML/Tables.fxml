<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXML.Tables">
   <children>
      <TableView fx:id="TablesView" prefHeight="400.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="Table" prefWidth="75.0" text="Table" />
          <TableColumn fx:id="Seats" prefWidth="75.0" text="Seats" />
            <TableColumn fx:id="Smoking" prefWidth="75.0" text="Smoking" />
            <TableColumn fx:id="Location" prefWidth="75.0" text="Location" />
            <TableColumn fx:id="Reserve" prefWidth="75.0" text="Reserve" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
